{
  "version": "1.0",
  "loop_settings": {
    "max_iterations_per_issue": 5,
    "require_build_pass": true,
    "require_typecheck_pass": true,
    "auto_commit": true,
    "commit_prefix": "feat"
  },
  "base_branch": "main",
  "commands": {
    "build": "npm run build",
    "typecheck": "npx tsc --noEmit",
    "lint": "npm run lint"
  }
}
